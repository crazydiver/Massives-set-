var n,a,b,c,i,i1:integer;
    mas:array [1..40] of longint;
begin
  i1:=1;
  readln(n);
  a:=1;
  mas[i1]:=a;
  write(mas[i1],' ');
  i1+=1;
  b:=1;
  mas[i1]:=b;
  write(mas[i1],' ');
  i1+=1;
  i:=1;
  while i<n do
    begin
      c:=a+b;
      mas[i1]:=c;
      write(mas[i1],' ');
      i1+=1;
      a:=b;
      b:=c;
      i:=i+1;
   end;
end.
